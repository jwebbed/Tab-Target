{
	"manifest_version": 2,

	"name": "Tab Target",
	"description": "Lets you browse the web with only a keyboard.",
	"version": "1.0",
	"options_page": "options.html",
	"permissions": [
		"tabs",
		"http://*/*",
		"https://*/*",
		"http://localhost/",
		"storage", "contextMenus"
	],
	"web_accessible_resources": ["search_bar.html"],
	"content_scripts" : [
    {
      "matches" : [
        "http://*/*",
        "https://*/*"
      ],
      "js" : ["jquery.js", "script.js", "jquery.highlight-4.js"],
      "css" : ["tt_style.css", "highlight.css"],
      "run_at" : "document_idle",
      "all_frames" : true
    }
  ]
}
