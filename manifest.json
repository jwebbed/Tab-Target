{
	"manifest_version": 2,

	"name": "Tab Target",
	"description": "Lets you browse the web with only a keyboard.",
	"version": "1.0",

	"permissions": [
		"tabs",
		"http://*/*",
		"https://*/*",
		"http://localhost/"
	],

	"content_scripts" : [
    {
      "matches" : [
        "http://*/*",
        "https://*/*"
      ],
      "js" : ["jquery.js", "script.js"],
      "css" : ["tt_style.css"],
      "run_at" : "document_idle",
      "all_frames" : true
    }
  ],

	"page_action":{
		"default_icon": "icon.png",
		"default_title": "Tab Target"
	}
}
